@model GorkemAydogduProje.Models.Database.tbl_Proje
@{
    ViewBag.Title = "ProjeGetir";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}
@using (Html.BeginForm("Guncelle", "Proje", FormMethod.Post))
{
<div class="form-group">
    @Html.LabelFor(m => m.Id)
    @Html.TextBoxFor(m => m.Id, new { @readonly = "true", @class = "form-control" })
    <br />
    @Html.LabelFor(m => m.ProjeAdi)
    @Html.TextBoxFor(m => m.ProjeAdi, new { @readonly = "true", @class = "form-control" })
    <br />
    @Html.LabelFor(m => m.Durumu)
    @Html.TextBoxFor(m => m.Durumu, new { @Value = "1", @readonly = "true", @class = "form-control" })

    <button style="margin-top:15px;" class="btn btn-warning">Güncelle</button>
</div>
}


